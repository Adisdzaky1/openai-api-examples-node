{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/apps/(.*)",
      "dest": "app.js"
    },
  
  {
      "src": "/apps/openai-funcs/(.*)",
      "dest": "app.js"
    
},
    {
      "src": "/apps/openai-tts/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/apps/openai-stt/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/apps/openai-moderation/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
